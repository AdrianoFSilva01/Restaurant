@screen md {
    .footer {
        .form {
            @apply w-4/6 flex-shrink-0;

            .nameInputs {
                @apply flex space-x-6 space-y-0;
            }
        }

        .contacts {
            @apply flex justify-center;

            .contactList {
                @apply flex-col justify-evenly content-center;

                .contactItem {
                    @apply items-center w-auto;
                }
            }
        }
    }
}
